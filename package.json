{
  "name": "@cenk1cenk2/eslint-config",
  "version": "0.0.0-development",
  "description": "Eslint configuration for my taste.",
  "main": "index.js",
  "repository": "https://github.com/cenk1cenk2/eslint-config",
  "author": "Cenk Kilic",
  "license": "MIT",
  "scripts": {
    "lint": "yarn lint:staged ./",
    "lint:staged": "eslint --ext .json,.ts,.js --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "src/*.{ts,js, json}": [
      "yarn lint:staged"
    ]
  },
  "peerDependencies": {
    "@typescript-eslint/parser": ">= 2.22.0",
    "eslint": ">= 6.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react-hooks": "^2.5.0"
  },
  "devDependencies": {
    "@cenk1cenk2/eslint-config": "0.0.28",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}