{
  "name": "@cenk1cenk2/eslint-config",
  "version": "3.2.0",
  "description": "Eslint configuration for my taste.",
  "main": "index.js",
  "repository": "git@gitlab.kilic.dev:config/eslint-config.git",
  "author": "Cenk Kilic",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./svelte": "./svelte.js",
    "./vue": "./vue.js",
    "./react": "./react.js",
    "./vue-typescript": "./vue-typescript.js",
    "./svelte-typescript": "./svelte-typescript.js",
    "./prettierrc": "./.prettierrc.js"
  },
  "scripts": {
    "format": "prettier --log-level warn --write ./*.js && eslint --fix",
    "lint": "eslint",
    "inspect": "eslint --inspect-config"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@vue/eslint-config-typescript": ">= 14.1.1",
    "eslint": ">= 9",
    "eslint-plugin-react": ">= 7",
    "eslint-plugin-react-hooks": ">= 4",
    "eslint-plugin-svelte": ">= 2",
    "eslint-plugin-vue": ">= 9",
    "svelte-eslint-parser": ">= 0.26.0"
  },
  "peerDependenciesMeta": {
    "@vue/eslint-config-typescript": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.3.0",
    "load-tsconfig": "^0.2.5",
    "typescript-eslint": "^8.56.1"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^2.2.5",
    "@vue/eslint-config-typescript": "^14.7.0",
    "eslint": "^10.0.2",
    "eslint-plugin-vue": "^10.8.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "simple-git-hooks": "^2.13.1",
    "svelte": "^5.53.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  },
  "packageManager": "pnpm@10.30.1"
}
