{
  "name": "@cenk1cenk2/eslint-config",
  "version": "3.1.21",
  "description": "Eslint configuration for my taste.",
  "main": "index.js",
  "repository": "git@gitlab.kilic.dev:config/eslint-config.git",
  "author": "Cenk Kilic",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./vue": "./vue.js",
    "./prettierrc": "./.prettierrc.js"
  },
  "scripts": {
    "format": "prettier --log-level warn --write ./*.js && eslint --fix",
    "lint": "eslint",
    "inspect": "eslint --inspect-config"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@vue/eslint-config-typescript": ">= 14.1.1",
    "eslint": ">= 9",
    "eslint-config-react": ">= 1",
    "eslint-plugin-react-hooks": ">= 4",
    "eslint-plugin-svelte": ">= 2",
    "eslint-plugin-vue": ">= 9",
    "svelte-eslint-parser": ">= 0.26.0"
  },
  "peerDependenciesMeta": {
    "@vue/eslint-config-typescript": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-config-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.14.0",
    "load-tsconfig": "^0.2.5",
    "typescript-eslint": "^8.20.0"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.8.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.17.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  },
  "packageManager": "pnpm@9.6.0"
}
